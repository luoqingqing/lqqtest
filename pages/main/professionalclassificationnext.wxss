page {
  background-color: #f5f5f5;
  height: 100%;
}

.space_view{
  height: 2rpx;
  background-color: #f5f5f5;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.content {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.content_item {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  flex-wrap: nowrap;
  padding: 20rpx;
  background-color: #fff;
}

.content_item_text{
  font-size: 30rpx;
  font-weight: 400;
  color: #333;
}

.content_item_img {
  width: 20rpx;
  height: 20rpx;
}

.prompt_text {
  font-size: 30rpx;
  font-weight: 400;
  padding-top: 200rpx;
  text-align: center;
  color: #999;
}
