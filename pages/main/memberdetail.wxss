page {
  background-color: #fff;
  height: 100%;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin-bottom: 100rpx;
}

.header_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-content: space-between;
  align-items: center;
  justify-content: space-between;
  justify-items: center;
  width: 750rpx;
  background-color: #fff;
}

.header_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  height: 450rpx;
}

.header_view_view_header_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  margin: 40rpx;
  align-content: space-between;
  align-items: center;
  justify-content: space-between;
  justify-items: center;
}

.header_view_view_header_view_img {
  width: 180rpx;
  height: 60rpx;
}

.header_view_view_header_view_state_text {
  font-size: 30rpx;
  color: #fff;
  font-weight: 400;
}

.header_view_view_content_view {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  width: 600rpx;
  margin: 60rpx;
}

.header_view_view_content_view_name_text {
  font-size: 40rpx;
  color: #fff;
  font-weight: 700;
}

.header_view_view_content_view_data_text {
  font-size: 30rpx;
  color: #fff;
  font-weight: 400;
}

.header_view_view_footer_view {
  display: flex;
  flex-direction: row-reverse;
  flex-wrap: nowrap;
  width: 680rpx;
  margin-bottom: 60rpx;
}

.header_view_view_footer_view_price_text {
  font-size: 38rpx;
  color: #fff;
   font-family:Arial;
  font-weight: 400;
}

.content_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
}

.content_view_yuan {
  width: 30rpx;
  height: 30rpx;
  background-color: #8187f5;
  margin-right: 10rpx;
  border-radius: 15rpx;
}

.content_view_view {
  display: flex;
  flex-direction: row;
  width: 100%;
  flex-wrap: wrap;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
}

.content_view_view_space_view {
  width: 80rpx;
  height: 2rpx;
  background-color: #f5f5f5;
  flex-grow: 1;
}

.content_view_view_space_view_two {
  width: 80rpx;
  height: 2rpx;
  background-color: #8187f5;
}

.content_view_view_tip_text {
  font-size: 36rpx;
  font-weight: 400;
  color: #666;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.content_view_view_two {
  position: relative;
  overflow: hidden;
  width: 670rpx;
  margin-top: 20rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.content_view_view_two_view {
  position: relative;
  float: left;
  height: 240rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
  width: 50%;
  border-width: 2rpx;
  border-style: solid;
  border-color: #fff;
  background-color: #f5f5f5;
  box-sizing: border-box;
}

.content_view_view_two_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
}

.content_view_view_two_view_title_text {
  font-size: 36rpx;
  font-weight: 600;
  color: #333;
}

.content_view_view_two_view_intro_text {
  font-size: 24rpx;
  font-weight: 400;
  margin-top: 10rpx;
  color: #666;
}

.content_view_view_view_tip_text {
  font-size: 24rpx;
  color: #666;
  font-weight: 400;
  margin-top: 20rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.content_tip_view {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  margin: 20rpx;
}

.content_tip_view_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-content: center;
  align-items: center;
}

.content_tip_view_view_big {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.content_tip_view_view_small {
  display: flex;
  flex-direction: row;
  align-content: center;
  flex-grow: 1;
}

.content_tip_view_view_big_text {
  font-size: 34rpx;
  font-weight: 400;
  color: #8187f5;
}

.content_tip_view_view_small_text {
  font-size: 26rpx;
  font-weight: 400;
  color: #666;
  margin-top: 10rpx;
  letter-spacing: 2rpx;
  margin-bottom: 20rpx;
}

.content_tip_view_view_intro {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  background-color: #fafafa;
  padding-bottom: 10rpx;
}

.content_tip_view_view_intro_text {
  font-size: 26rpx;
  font-weight: 400;
  color: #999;
  margin-top: 10rpx;
  margin-left: 20rpx;
  letter-spacing: 2rpx;
}

.content_tip_view_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
}

.content_tip_view_view_view_text {
  font-size: 30rpx;
  font-weight: 400;
  color: #fff;
  border-radius: 10rpx;
  background-color: #aca9f1;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.content_tip_view_view_view_img {
  width: 25rpx;
  height: 22rpx;
}

.footer_view {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: #fff;
  position: fixed;
  align-content: space-around;
  align-items: center;
  justify-content: space-around;
  justify-items: center;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  height: 100rpx;
  bottom: 0rpx;
  right: 0rpx;
  left: 0rpx;
}

.footer_view_phonenum_text {
  width: 300rpx;
  font-size: 40rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  color: #fff;
  text-align: center;
  border-radius: 40rpx;
  background-color: #7b80f6;
  font-weight: 600;
}

.footer_view_purchase_text {
  font-size: 40rpx;
  color: #fff;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-radius: 40rpx;
  background-color: #fe8ba8;
  width: 300rpx;
  text-align: center;
  font-weight: 600;
}
/* 弹窗 */
.dialogVisible_tip_text {
  font-size: 30rpx;
  color: #666;
  font-weight: 400;
}