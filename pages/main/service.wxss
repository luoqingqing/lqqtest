page {
  background-color: #fff;
  height: 100%;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.header {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  height: 100rpx;
  position: fixed;
  top: 0rpx;
  right: 0rpx;
  left: 0rpx;
}

.content {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.content_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin-top: 100rpx;
}

.content_item_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: center;
  justify-items: center;
  align-content: center;
  align-items: center;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
  padding: 20rpx;
   box-shadow: 0rpx 2rpx 2rpx 2rpx #f5f5f5; /*for Android*/
  -webkit-box-shadow: 0px 2px 2px 2px #f5f5f5; /*for IOS*/
}

.content_item_view_img {
  width: 120rpx;
  height: 120rpx;
}

.content_item_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin-left: 20rpx;
  flex-grow: 1;
}

.content_item_view_title {
  font-size: 38rpx;
  font-weight: 600;
  color: #333;
}

.content_item_view_intro {
  font-size: 26rpx;
  font-weight: 400;
  color: #666;
}

.dialogVisible_tip_text {
  font-size: 30rpx;
  color: #666;
  font-weight: 400;
}
