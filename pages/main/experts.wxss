page {
  background-color: #f5f5f5;
  height: 100%;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.banner_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  position: fixed;
  background-color: #fff;
  top: 0rpx;
  right: 0rpx;
  left: 0rpx;
}

.slide_image {
  width: 100%;
  height: 420rpx;
}

.header {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  height: 100rpx;
  position: fixed;
  top: 420rpx;
  right: 0rpx;
  left: 0rpx;
}

.space_view {
  height: 2rpx;
  background-color: #f5f5f5;
}

.content {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  margin-top: 520rpx;
}

.content_item {
  display: flex;
  flex-direction: row;
  margin-top: 2rpx;
  flex-wrap: nowrap;
  background-color: #fff;
  padding: 20rpx;
}

.content_item_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.content_item_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  margin-left: 20rpx;
  flex-grow: 1;
}

.content_item_view_img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  border: 2rpx solid #fff;
}

.content_item_view_next_img {
  width: 20rpx;
  height: 20rpx;
}

.content_item_view_name_text {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
}

.content_item_view_degree_text {
  font-size: 30rpx;
  font-weight: 400;
  margin-top: 10rpx;
  color: #666;
}

.prompt_text {
  font-size: 30rpx;
  font-weight: 400;
  padding-top: 600rpx;
  text-align: center;
  color: #999;
}
