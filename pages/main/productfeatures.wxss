page {
  background-color: #fff;
  height: 100%;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  background-color: #fff;
}

.tip_view {
  background-color: #f5f5f5;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-content: center;
  align-items: center;
}

.tip_view_view {
  width: 10rpx;
  height: 40rpx;
  background-color: #8087f5;
}

.tip_view_text {
  font-size: 36rpx;
  color: #333;
  margin-left: 20rpx;
  font-weight: 600;
}

.text {
  font-size: 30rpx;
  font-weight: 400;
  color: #666;
  padding: 20rpx;
}
