page {
  background-color: #f5f5f5;
  height: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.sc_view {
  background-color: #fff;
}

.parent {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.space_view {
  width: 100%;
  height: 2rpx;
  background-color: #f5f5f5;
}

.header_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.header_img {
  width: 100%;
  height: 400rpx;
}

.header_view_view {
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: nowrap;
}

.sc {
  display: flex;
  white-space: nowrap;
  flex-direction: row;
  flex-wrap: wrap;
  width: 100%;
}

.header_view_text {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
  margin: 20rpx;
}

.header_view_img {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.header_evaluation_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.header_evaluation_details_view {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background-color: #f5f5f5;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.item_left_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  padding: 20rpx;
}

.item_right_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  border-top-right-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  align-content: center;
  align-items: center;
  justify-content: center;
  justify-items: center;
}

.space_view {
  height: 2rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background-color: #f5f5f5;
}

.item_view_text {
  font-size: 34rpx;
  font-weight: 600;
  color: #6d6e8c;
}

.item_view_img {
  width: 22rpx;
  height: 46rpx;
}

.item_view_tip_text {
  font-size: 24rpx;
  font-weight: 400;
  clear: #fff;
}

.content_view_text {
  font-size: 26rpx;
  font-weight: 400;
  color: #9d9fb7;
}

.dialogVisible_tip_text {
  font-size: 30rpx;
  color: #666;
  font-weight: 400;
}

.content_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.content_view_flag_view {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 20rpx;
  margin-left: 20rpx;
  align-items: center;
}

.content_view_flag_view_space {
  width: 10rpx;
  height: 30rpx;
  background-color: #8087f5;
}

.content_view_flag_view_text {
  font-size: 26rpx;
  font-weight: 400;
  color: #666;
  margin-left: 20rpx;
}

.content_lesson_introduction_text {
  font-size: 30rpx;
  font-weight: 400;
  color: #333;
  margin: 20rpx;
}

.timetable_view {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 20rpx;
}

.timetable_view_view {
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
}

.timetable_not_selected_text {
  font-size: 30rpx;
  font-weight: 400;
  border-radius: 2%;
  padding: 10rpx;
  color: #8087f5;
  margin-right: 20rpx;
  background: #fff;
  margin-top: 20rpx;
  border: solid #8087f5 2rpx;
}

.timetable_selected_text {
  font-size: 30rpx;
  font-weight: 400;
  border-radius: 2%;
  padding: 10rpx;
  color: #fff;
  margin-right: 20rpx;
  margin-top: 20rpx;
  background: #8087f5;
  border: solid #8087f5 2rpx;
}

.timetable_non_appointment {
  font-size: 30rpx;
  font-weight: 400;
  border-radius: 2%;
  padding: 10rpx;
  color: #fff;
  margin-right: 20rpx;
  margin-top: 20rpx;
  background: #f5f5f5;
  border: solid #f5f5f5 2rpx;
}

.content_frequency_service_text {
  font-size: 30rpx;
  font-weight: 400;
  color: #f00;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.content_lesson_introduction_text {
  font-size: 30rpx;
  font-weight: 400;
  color: #333;
  margin: 20rpx;
  letter-spacing: 2rpx;
}

.footer_button {
  text-align: center;
  background-color: #8087f5;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
  font-size: 30rpx;
  color: #fff;
  margin: 40rpx;
}

.btn_hover {
  background-color: #f5f5f5;
}
